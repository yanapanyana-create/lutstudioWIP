{
  "name": "lut-studio-ai",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "electron:dev": "NODE_ENV=development electron .",
    "electron:build": "npm run build && electron-builder"
  },
  "dependencies": {
    "@google/genai": "^1.39.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^31.0.0",
    "electron-builder": "^24.13.3",
    "typescript": "^5.7.2",
    "vite": "^6.0.0"
  },
  "build": {
    "appId": "com.lutstudio.ai",
    "productName": "LutStudio AI",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "mac": {
      "category": "public.app-category.photography",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "portable"
      ]
    }
  }
}